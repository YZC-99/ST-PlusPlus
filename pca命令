CUDA_VISIBLE_DEVICES=0,1 torchrun --nproc_per_node=2 ddp_main_uda.py \
  --dataset od_val_refuge --data-root ./data/fundus_datasets/od_oc/WACV/REFUGE_cross_new/ \
  --batch-size 4 --backbone resnet50 --model deeplabv3plus \
  --labeled-id-path dataset/splits/$semi_setting/labeled.txt \
  --unlabeled-id-path dataset/splits/$semi_setting/unlabeled.txt \
  --pseudo-mask-path experiments_pca/pseudo_masks/$semi_setting \
  --save-path experiments_pca/models/$semi_setting \
  --epochs 150 \
  --lr 0.001

