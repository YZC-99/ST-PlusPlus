  --dataset od_val_refuge --data-root ./data/fundus_datasets/od_oc/WACV/REFUGE_cross_new/ \
  --batch-size 16 --backbone resnet50 --model deeplabv3plus \
  --labeled-id-path dataset/splits/$semi_setting/labeled.txt \
  --unlabeled-id-path dataset/splits/$semi_setting/unlabeled.txt \
  --pseudo-mask-path experiments_pca/pseudo_masks/od_val_refuge/1_7/100refuge \
  --tgt-pseudo-mask-path experiments_pca/$now_date$description/tgt_pseudo_masks/$semi_setting \
  --save-path experiments_pca/$now_date$description/models/$semi_setting \
  --epochs 150 \
  --stage5 True \
  --lr 0.004




